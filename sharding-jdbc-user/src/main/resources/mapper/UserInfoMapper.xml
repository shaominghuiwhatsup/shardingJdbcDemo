<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shardingsphere.demo.mapper.UserInfoMapper">

    <insert id="insert" parameterType="com.shardingsphere.demo.mapper.entity.UserInfoEntity">
        insert into user_info (phone, addr)
        values (#{phone}, #{addr})
    </insert>

</mapper>